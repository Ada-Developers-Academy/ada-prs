<%= form_for(@repo, html: { class: "form-horizontal" }) do |f| %>
  <fieldset>
    <legend>Repositiory Properties</legend>
    <div class="control-group">
      <%= f.label :cohort_num %>
      <%= f.text_field :cohort_num, class: "form-control" %>
    </div>
    <div class="control-group">
      <%= f.label :repo_url %>
      <%= f.text_field :repo_url, class: "form-control" %>
    </div>
    <div class="control-group">
      <%= f.label :indiviudal %>
      <%= f.check_box :individual, class: "form-control" %>
    </div>
  </fieldset>
  <div class="control-group">
    <%= f.submit %>
  </div>
<% end %>
