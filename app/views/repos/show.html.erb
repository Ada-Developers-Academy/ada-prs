<h1><%= @repo.repo_url %></h1>
<h4><%= link_to "Github", "http://github.com/" + @repo.repo_url + "/pulls", :target => "_blank" %></h4>
<table class="table">
  <thead>
    <th>Student Name</th>
    <th>GitHub Name</th>
    <th>Submitted</th>
    <!-- <th></th> -->
  </thead>
  <tbody>
    <% @all_data.each do |stud| %>
      <tr class='<%= !stud[:created_at] ? "danger" : "success" %>' >
      <td><%= stud[:student].name.capitalize %></td>
      <td><%= stud[:student].github_name %></td>
      <% if !stud[:created_at] %>
        <td><%= mail_to stud[:student][:email], "Send Notification", cc: "TMP",
         subject: @repo.repo_url %></td>
       <% else %>
        <td><%= link_to localize(stud[:created_at], :format => :short), stud[:url], target: "_blank" if stud[:created_at] %></td>
       <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
